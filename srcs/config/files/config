server {
    listen 80
    error_page 707 /helloworlderror/
    server_name example
    cgi_param   hello world
    cgi_pass    localhost:9000
    location /blabla {
        root /tmpBlabla/
        error_page  404 /err/here
        allow_methods POST LIVE
        index blabla_index.html
        autoindex on
        alias /alias/
        }
    location /hello {
        root /world/
    }
}

server {
    listen 8000
    listen 80
    server_name example2
}

server {
    listen 253.0.127.2:999
    listen 666
    listen 8000
    listen 127.0.0.1:80
}